{
    "hide-files.files": [
        ".github",
        ".vscode",
        "tests",
        ".pre-commit-config.yaml",
        "LICENSE",
        "meta.yaml",
        "pyproject.toml",
        "pytest.ini",
        ".coveragerc",
        ".flake8"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/*.4ct": true,
        "**/*.4tc": true,
        "**/*.fdb_latexmk": true,
        "**/*.idv": true,
        "**/*.lg": true,
        "**/*.lof": true,
        "**/*.synctex.gz": true,
        "**/*.synctex(busy)": true,
        "**/*.tmp": true,
        "**/*.xref": true,
        "**/*.fls": true,
        "**/*.loc": true,
        "**/*.run.xml": true,
        "**/*.soc": true,
        "**/*.snm": true,
        "**/*.blg": true,
        "**/*-SAVE-ERROR": true,
        ".github": true,
        ".vscode": true,
        "RosettaX.egg-info": true,
        "tests": true,
        ".pre-commit-config.yaml": true,
        "LICENSE": true,
        "meta.yaml": true,
        "pyproject.toml": true,
        "pytest.ini": true,
        ".coveragerc": true,
        ".flake8": true
    }
}